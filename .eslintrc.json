{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": [
        "error",
        "ignorePackages",
        {
            "ts": "never"
        }
        ],
        "no-console": "warn",
        "no-underscore-dangle": ["error", { "allow": ["_id"] }],
        "no-restricted-syntax": 0,
        "no-await-in-loop": 0
          
    },
    "settings": {
        "import/resolver": {
          "typescript": {
    
          }
        }
      }
}
